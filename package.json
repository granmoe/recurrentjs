{
  "name": "recurrentjs",
  "version": "1.0.0",
  "description": "RecurrentJS is a Javascript library that implements:",
  "main": "analysis.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/granmoe/recurrentjs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/granmoe/recurrentjs/issues"
  },
  "homepage": "https://github.com/granmoe/recurrentjs#readme",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.(js|json|md)": [
      "prettier --write",
      "xo --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "react": {
      "version": "^16.2.0"
    },
    "prettier": true,
    "space": true,
    "rules": {
      "no-void": "off",
      "react/require-default-props": "off",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ],
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true
        }
      ]
    },
    "global": [
      "describe",
      "test",
      "xtest",
      "expect",
      "jest",
      "React"
    ]
  },
  "dependencies": {
    "@zeit/next-workers": "^1.0.0",
    "next": "v7.0.2-canary.50",
    "promise-worker": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "recharts": "^1.4.1",
    "rnn": "^0.0.2",
    "styled-components": "^4.1.3",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^5.11.1",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.12.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "xo": "^0.23.0"
  }
}
